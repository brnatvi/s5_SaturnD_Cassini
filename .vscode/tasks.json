{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/client-request.c",
                "${workspaceFolder}/src/listd.c",
                "${workspaceFolder}/src/saturnd.c",
                "${workspaceFolder}/src/timing-text-io.c",
                "-I",
                "${workspaceFolder}/include/",
                "-o",
                "${workspaceFolder}/saturnd"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}